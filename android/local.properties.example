# Local Android SDK path (Android Studio usually writes this automatically)
# sdk.dir=/Users/<you>/Library/Android/sdk

# Required Supabase URL (e.g. https://<project-id>.supabase.co)
SUPABASE_URL=

# Optional debug override for local Supabase (default: SUPABASE_URL)
# SUPABASE_URL_DEBUG=http://10.0.2.2:54321

# Supabase anon key for client SDK usage
SUPABASE_ANON_KEY=

# Cloudflare Gateway proxy endpoint
GATEWAY_URL=

# Optional debug override when hitting Supabase locally (default: GATEWAY_URL)
# GATEWAY_URL_DEBUG=http://10.0.2.2:54321

# Google OAuth client ID for web (used by Google Sign-In)
WEB_GOOGLE_CLIENT_ID=

# Stripe publishable key
STRIPE_PUBLISHABLE_KEY=
STRIPE_PUBLISHABLE_KEY_DEBUG=
